{
  "name": "skill",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "sync": "rm -rf dist/data/public && scp -r hj:/root/Workspace/skill/data/public dist/data/public",
    "push": "tar czf public.tar.gz dist/public && scp public.tar.gz hj:/root/Workspace/skill/ && rm -rf public.tar.gz",
    "release": "npm run build && npm run push && ssh hj fe_deploy",
    "refs": "replacer src/** -m antd -m mora-scripts -m mora-common",
    "ck:css": "sass --sourcemap=none --no-cache plugin/ckeditor/contents.scss public/ckeditor/contents.css",
    "ck:js": "cp plugin/ckeditor/config.js public/ckeditor/config.js",
    "ck": "npm run ck:css && npm run ck:js",
    "dev": "webpack-dev-server --hot",
    "dev:admin": "PRODUCT=admin PORT=8001 npm run dev",
    "dev:neo": "PRODUCT=neo PORT=9000 npm run dev",
    "build": "NODE_ENV=production webpack -p",
    "build:admin": "PRODUCT=admin npm run build"
  },
  "author": "Mora <qiuzhongleiabc@126.com> (https://qiu8310.github.io/)",
  "license": "MIT",
